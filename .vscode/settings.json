{
    "telemetry.enableTelemetry": false,
    "telemetry.feedback.enabled": false,
    "telemetry.telemetryLevel": "off",
    "terminal.integrated.allowChords": false,
    "editor.accessibilitySupport": "off",
    "editor.autoClosingBrackets": "always",
    "editor.autoClosingComments": "always",
    "editor.autoClosingQuotes": "always",
    "editor.autoSurround": "brackets",
    "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
    "editor.codeActionsOnSave": {},
    "editor.fontSize": 15,
    "editor.fontFamily": "'Fira Code', 'NoTo Sans Mono', monospace",
    "editor.tabSize": 6,
    "editor.cursorStyle": "block-outline",
    "editor.wordWrap": "wordWrapColumn",
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true,
    "diffEditor.codeLens": true,
    "diffEditor.diffAlgorithm": "legacy",
    "diffEditor.experimental.useTrueInlineView": true,
    "workbench.activityBar.iconClickBehavior": "focus",
    "workbench.activityBar.location": "bottom",
    "workbench.colorTheme": "GitHub Dark Default",
    "workbench.preferredDarkColorTheme": "Monokai",
    "workbench.preferredLightColorTheme": "Quiet Light",
    "workbench.iconTheme": "vs-minimal",
    "breadcrumbs.enabled": true,
    "editor.minimap.autohide": true,
    "workbench.settings.editor": "json",
    "window.autoDetectColorScheme": false,
    "ipynb.pasteImagesAsAttachments.enabled": false,
    "javascript.preferGoToSourceDefinition": true,
    "javascript.referencesCodeLens.enabled": true,
    "javascript.referencesCodeLens.showOnAllFunctions": true,
    "npm.enableRunFromFolder": true,
    "npm.packageManager": "npm",
    "javascript.inlayHints.functionLikeReturnTypes.enabled": true,
    "typescript.tsserver.experimental.enableProjectDiagnostics": true,
    "typescript.tsserver.maxTsServerMemory": 2024
}